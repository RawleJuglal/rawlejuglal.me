<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title><%= title %></title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--Custom CSS-->
    <link rel="stylesheet" href='/stylesheets/codecademy/luchaUnderground.css'>
    <!-- Latest compiled and minified JavaScript -->
    <!--JQuery-->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <!--Bootstrap-->
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src='/bower_components/angular/angular.min.js'></script>
  </head>
  <body ng-app='codecademy'>
  <div class="header">
      <div class="container">
        <img src="data:image/jpeg;base64," width="90" height="34">
      </div><!--End of container-->
    </div><!--End of header-->

    <div class="main" ng-controller="LuchaController">
      <div class="container">
        <div class="content">
          <div class="row">
            <div class="col-sm-4 col-md-3" class="series_img">
                <img ng-src="{{program.series_img}}"/>
            </div><!--End of col-md-3-->
            <div class="col-sm-8 col-md-6">
              <h1 ng-bind='::program.series' class="series"></h1>
              <h2 ng-bind='::program.episode' class="episode"></h2>
              <p ng-bind='::program.description' class="description"></p>
            </div><!--End of col-md-6-->
            <div class="col-sm-12 col-md-3">
              <ul class="list-group">
                <li class="list-group-item"><span>Date:</span> {{program.datetime | date }} </li>
                <li class="list-group-item"><span>On air:</span> {{program.datetime | date:'EEEE'}} </li>
                <li class="list-group-item"><span>Time:</span> {{program.datetime | date:'h:mm a'}} </li>
                <li class="list-group-item"><span>Season:</span>{{program.season}}  </li>
                <li class="list-group-item"><span>Genre:</span>{{program.genre}}  </li>
              </ul>
            </div><!--End of col-md-3-->
          </div><!--End of row-->
        </div><!--End of content-->
      </div><!--End of container-->
    </div><!--End of main-->

    <div class="footer">
      <div class="container">
        <div class="row text-center">
          <div class="col-xs-4 col-md-1 col-md-offset-4">
            <h3>El Rey</h3>
            <ul>
              <li>Careers</li>
              <li>Terms</li>
              <li>Help</li>
            </ul>
          </div><!--End of col-md-3-->
          <div class="col-xs-4 col-md-2">
            <h3>More El Rey</h3>
            <ul>
              <li>Gift Cards</li>
              <li>Trailers</li>
            </ul>
          </div><!--End of col-md-3-->
          <div class="col-xs-4 col-md-1">
            <h3>News</h3>
            <ul>
              <li>Blog</li>
              <li>Twitter</li>
              <li>YouTube</li>
              <li>Google+</li>
              <li>Facebook</li>
            </ul>
          </div><!--End of col-md-3-->
        </div><!--End of row-->
      </div><!--End of container-->
    </div><!--End of footer-->
    <!--Module-->
    <script src="/ang.js"></script>
    <!--Controller-->
    <script src='/Controllers/codecademy/luchaController.js'></script>
    <!--Custom-->
    <script src='/javascripts/codecademy/luchaUnderground.js'></script>
  </body>
</html>