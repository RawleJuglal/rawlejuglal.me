    <title><%= title %></title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--Custom CSS-->
    <link href='https://fonts.googleapis.com/css?family=Oswald:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href='/stylesheets/codecademy/Move.css'>
    <!-- Latest compiled and minified JavaScript -->
    <!--JQuery-->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <!--Bootstrap-->
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src='/bower_components/angular/angular.min.js'></script>
  </head>
  <body>
    <div class="main">
      <div class="container">
          <div class='row'>
              <div class='col-xs-12 text-center'>
                    <h1>Move</h1>
                    <p>Form healthy habits to take your fitness to the next level.</p>
                    <a href='#' class='btn btn-default btn-move'>START NOW</a>
              </div><!--End of col-xs-12-->
          </div><!--End of row-->
      </div><!--End of container-->
    </div><!--End of main-->
    <div class='supporting'>
        <div class='container'>
            <div class='row'>
                <div class='col-xs-12 col-sm-4 text-center'>
                  <h2>Move</h2>
                  <p>Become more active by tracking your runs, rides, and walks.</p> 
                </div><!--End of col-xs-12-->
                <div class='col-xs-12 col-sm-4 text-center'>
                  <h2>Sync</h2>
                  <p>Access your activity on your phone, tablet, or computer.</p> 
                </div><!--End of col-xs-12-->
                <div class='col-xs-12 col-sm-4 text-center'>
                  <h2>Compete</h2>
                  <p>Set personal challenges and see how you rank against your friends.</p>  
                </div><!--End of col-xs-12-->
            </div><!--End of row-->
        </div><!--End of container-->
    </div><!--End of supporting-->
    <div class='feature'>
        <div class='container'>
            <div class='row'>
                <div class='col-xs-12 text-center'>
                   <p>MOVE. REST. RECOVER. MOVE.</p> 
                </div><!--End of col-xs-12-->
            </div><!--End of row-->
        </div><!--End of container-->
    </div><!--End of feature-->
    <div class='supporting'>
        <div class='container'>
            <div class='row'>
                <div class='col-xs-12 text-center'>
                    <h2>GO PREMIUM</h2>
                    <p>Receive recommendations based on your activity to level up.</p>
                    <a href="#" class='btn btn-default btn-move2'>LEARN MORE</a>
                </div><!--End of col-xs-12-->
            </div><!--End of row-->
        </div><!--End of container-->
    </div><!--End of supporting-->
    <div class='footer'>
        <div class='container'>
            <div class='row'>
                <div class='col-xs-12 text-center'>
                    <p>STOP SCROLLING. START MOVING</p>
                    <a href="#" class='btn btn-default btn-move'>START NOW</a> 
                </div><!--End of col-xs-12-->
            </div><!--End of row-->
        </div><!--End of container-->
    </div><!--End of footer-->
