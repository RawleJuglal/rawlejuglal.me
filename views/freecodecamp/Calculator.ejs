<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title><%= title %></title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--Custom CSS-->
    <link rel="stylesheet" href='/stylesheets/freecodecamp/calculator.css'>
    
  </head>
  <body>
      <div class='heading'>
        <div class='container'>
            <div class='row'>
                <div class='col-xs-12 text-center'>
                    <h1>FreeCodeCamp</h1>
                    <h2>Electronic Calculator</h2>
                </div><!--End of col-xs-12-->
            </div><!--End of row-->
        </div><!--End of container-->  
      </div><!--End of heading-->
      <div class='calculatorDiv'>
          <div class='container'>
              <div class='row'>
                  <div class='col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4'>
                    <div class='calculator'>
                      <div class='row'>
                          <div class='col-xs-12'>
                              <div id='outputBox' class='panel panel-default'>
                                <div class="panel-body">
                      
                                </div><!--End of panel-body-->  
                              </div><!--End of outputBox-->
                          </div><!--End of col-xs-12-->
                      </div><!--End of row-->
                      <div class='row interface'>
                          <div class='col-xs-3'>
                            <div id="acButton-container">
                                <button id="acButton" class="btn btn-default btn-block">AC</button>
                            </div><!--End of acButton-container-->  
                          </div><!--End of col-xs-3-->
                          <div class='col-xs-3'>
                            <div id="ceButton-container">
                                <button id="ceButton" class="btn btn-default btn-block">CE</button>
                            </div><!--End of ceButton-container-->  
                          </div><!--End of col-xs-3-->
                          <div class='col-xs-3'>
                            <div id="percentButton-container">
                                <button id="percentButton" class="btn btn-default btn-block">%</button>
                            </div><!--End of percentButton-container-->  
                          </div><!--End of col-xs-3-->
                          <div class='col-xs-3'>
                            <div id="divideButton-container">
                                <button id="divideButton" class="btn btn-default btn-block">/</button>
                            </div><!--End of divideButton-container-->  
                          </div><!--End of col-xs-3-->
                      </div><!--End of row-->
                      <div class='row interface'>
                        <div class='col-xs-3'>
                         <div id="sevenButton-container">
                            <button id="sevenButton" class="btn btn-default btn-block">7</button>
                          </div><!--End of sevenButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                          <div id="eightButton-container">
                            <button id="eightButton" class="btn btn-default btn-block">8</button>
                          </div><!--End of eightButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                          <div id="nineButton-container">
                            <button id="nineButton" class="btn btn-default btn-block">9</button>
                          </div><!--End of nineButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                         <div id="multiplyButton-container">
                            <button id="multiplyButton" class="btn btn-default btn-block">X</button>
                          </div><!--End of multiplyButton-->
                        </div><!--End of col-xs-3-->
                      </div><!--End of row-->
                      <div class='row interface'>
                        <div class='col-xs-3'>
                         <div id="fourButton-container">
                            <button id="fourButton" class="btn btn-default btn-block">4</button>
                          </div><!--End of fourButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                          <div id="fiveButton-container">
                            <button id="fiveButton" class="btn btn-default btn-block">5</button>
                          </div><!--End of fiveButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                          <div id="sixButton-container">
                            <button id="sixButton" class="btn btn-default btn-block">6</button>
                          </div><!--End of sixButton-->
                        </div><!--End of col-xs-3-->
                        <div class='col-xs-3'>
                         <div id="minusButton-container">
                            <button id="minusButton" class="btn btn-default btn-block">-</button>
                          </div><!--End of minusButton-->
                        </div><!--End of col-xs-3-->
                      </div><!--End of row-->
                      <div class='row'>
                          <div class='col-xs-9'>
                            <div class='row interface'>
                              <div class='col-xs-4'>
                                <div id="oneButton-container">
                                    <button id="oneButton" class="btn btn-default btn-block">1</button>
                                </div><!--End of oneButton-->
                              </div><!--End of col-xs-4-->
                              <div class='col-xs-4'>
                                <div id="twoButton-container">
                                    <button id="twoButton" class="btn btn-default btn-block">2</button>
                                </div><!--End of twoButton-->
                              </div><!--End of col-xs-4-->
                              <div class='col-xs-4'>
                                <div id="threeButton-container">
                                    <button id="threeButton" class="btn btn-default btn-block">3</button>
                                </div><!--End of threeButton-->
                              </div><!--End of col-xs-4-->
                            </div><!--End of row-->
                            <div class='row interface'>
                              <div class="col-xs-4">
                                <div id="zeroButton-container">
                                    <button id="zeroButton" class="btn btn-default btn-block">0</button>
                                </div><!--End of zeroButton-->
                              </div><!--End of col-xs-4-->
                              <div class="col-xs-4">
                                <div id="periodButton-container">
                                    <button id="periodButton" class="btn btn-default btn-block">.</button>
                                </div><!--End of twoButton-->
                              </div><!--End of col-xs-4-->
                              <div class="col-xs-4">
                                <div id="equalButton-container">
                                    <button id="equalButton" class="btn btn-default btn-block">=</button>
                                </div><!--End of equalButton-->
                              </div><!--End of col-xs-4--> 
                            </div><!--End of row-->
                          </div><!--End of col-xs-9-->
                          <div class='col-xs-3'>
                            <div id="plusButton-container">
                                <button id="plusButton" class="btn btn-default btn-block">+</button>
                            </div><!--End of plusButton-->  
                          </div><!--End of col-xs-3-->
                      </div><!--End of row-->
                  </div><!--End of calculator-->  
                  </div><!--End of col-xs-12-->
              </div><!--End of row-->
          </div><!--End of container-->
      </div><!--End of calculatorDiv-->
      <!-- Latest compiled and minified JavaScript -->
    <!--JQuery-->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <!--Bootstrap-->
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!--<script src='/bower_components/angular/angular.min.js'></script>-->
    <!--Custom-->
    <script type='text/javascript' src='/javascripts/freecodecamp/calculator.js'></script>
  </body>
</html>