<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title><%= title %></title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--Custom CSS-->
    <link rel="stylesheet" href='/stylesheets/codecademy/slotMachine.css'>
    <!-- Latest compiled and minified JavaScript -->
    <!--JQuery-->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <!--Bootstrap-->
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src='/bower_components/angular/angular.min.js'></script>
  </head>
  <body>
    <div class='header'>
      <div class='container-fluid'>
          <div class='row'>
              <div class='col-xs-12'>
                <img class='center-block' src='https://s3.amazonaws.com/freecodecamp/freecodecamp_logo.svg.gz'></img>
              </div><!--End of col-xs-12-->
              <div class='col-xs-12 text-center'>
                <h2>FCC Slot Machine</h2>  
              </div><!--End of col-xs-12-->
          </div><!--End of row-->
      </div><!--End of container-fluid-->  
    </div><!--End of header-->
    <div class='slotMachineDiv'>
        <div class='container-fluid'>
            <div class='row'>
                <div class='col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4 slotMachine'>
                   <div class='slotDiv'>
                    <div class='container-fluid'>
                        <div class='row'>
                            <div class='col-xs-12 slots'>
                                <div class='row'>
                                    <div class='col-xs-4 slot'>
                                        <img class='center-block' src='/images/large-white-background.jpg'/>
                                    </div><!--End of col-lg-4-->
                                    <div class='col-xs-4 slot'>
                                        <img class='center-block' src='/images/large-white-background.jpg'/>
                                    </div><!--End of col-lg-4-->
                                    <div class='col-xs-4 slot'>
                                        <img class='center-block' src='/images/large-white-background.jpg'/>
                                    </div><!--End of col-lg-4-->
                                </div><!--End of row-->
                            </div><!--End of col-lg-4-->
                        </div><!--End of row-->
                    </div><!--End of container-fluid-->
                </div><!--End of slotDiv-->
                <div class='loggerDiv'>
                    <div class='container-fluid'>
                        <div class='row'>
                            <div class='col-xs-12 logger'>
                                <p class='text-center'>You've got to spin to win!</p>
                            </div><!--End of col-xs-12-->    
                        </div><!--End of row-->
                    </div><!--End of container-fluid-->
                </div><!--End of logger-->
                <div class='spinnerDiv'>
                    <div class='container-fluid'>
                        <div class='row'>
                            <div class='col-xs-12 spinner text-center'>
                                <button class='btn btn-default btn-slot'>Spin</button>
                            </div><!--End of spinner-->
                        </div><!--End of row-->
                    </div><!--End of container-fluid-->
                </div><!--End of spinnerDiv--> 
                </div><!--End of slotMachine-->
            </div><!--End of row-->
        </div><!--End of container-fluid-->
    </div><!--End of slotMachine-->
    <!--Custom-->
    <script src='/javascripts/freecodecamp/slotMachine.js'></script>
  </body>
</html>